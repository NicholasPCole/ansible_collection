au BufRead,BufNewFile *.gs set filetype=javascript
au BufRead,BufNewFile *.md set filetype=markdown
filetype indent on
filetype on
filetype plugin on
set autoindent
set background=dark
set expandtab
set foldlevel=1
set foldmethod=manual
set foldnestmax=10
set hls
set mat=5
set nocompatible
set nofoldenable
set number
set ruler
set shiftwidth=4
set showmatch
set showmode
set smartindent
set tabstop=4
syntax on
